<?xml version="1.0" encoding="utf-8" ?> 
<configuration>
  <configSections>
    <section name="rabnetds" type="rabnet.RabnetConfigHandler,rabnet"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net"/>
  </configSections>
  <rabnetds>
    <dataSource default="1" user="зоотехник" name="Main" type="db.mysql" param="host=localhost;uid=kroliki;pwd=krol;database=kroliki100;charset=utf8"/>
    <dataSource user="зоотехник" name="trust" type="db.mysql" param="host=10.5.157.108;uid=kroliki;pwd=krol;database=kroliki;charset=utf8"/>
    <dataSource user="зоотехник" name="Trial" type="db.mysql" param="host=localhost;uid=kroliki;pwd=krol;database=krol_trial;charset=utf8"/>
  </rabnetds>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger{2} [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="log.txt" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger{2} [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>
</configuration>
